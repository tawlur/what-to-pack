
<%- include('../partials/header') %>
<h2>Your Trips</h2>

<div>
  <a href="/trips/new">Add New Trip</a>
</div>
  
<div class="mainContent">
    <table>
      <thead class="itemTable">
        <th>Location</th>
        
      </thead>
      <tbody>
        <% trips.forEach(function(i) { %>
          <td><a href="/trips/<%= i._id %>"> <%= i.location %> </a></td>
          
        </tbody>
        <% }); %>
      </table>
      
    </div>
    
    <%- include('../partials/footer'); %>